{
    "plugins": ["plugins/markdown"],
    "markdown": {
        "tags" : ["module"]
    },
    "recurseDepth": 10,
    "source": {
        "exclude": ["2.0.0/out"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}